{
  "name": "simple-testing",
  "version": "1.0.0",
  "description": "A very simple implementation of React that is being served up by express and webpack and ReactDOM/Server.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "start": "node ./dist/server",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-jest": "^25.1.0",
    "jest": "^25.1.0",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest/setupTestFramework.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16.3": "^1.7.2",
    "express": "^4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "radium": "^0.26.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "webpack": "^4.42.0",
    "webpack-node-externals": "^1.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmshanahan/ReactExpressServer.git"
  },
  "bugs": {
    "url": "https://github.com/jmshanahan/ReactExpressServer/issues"
